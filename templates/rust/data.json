{
  "workdir": "{{ inputs.rust_workdir }}",
  "variables": {
    "inputs": [
      {
        "name": "rust_workdir",
        "default": "./",
        "desc": "Specify all of rust steps where are run",
        "type": "string",
        "required": "false"
      },
      {
        "name": "container_workdir",
        "default": "./",
        "desc": "Specify all of container steps where are run",
        "type": "string",
        "required": "false"
      }
    ],
    "secrets": [],
    "outputs": []
  },
  "lint": {},
  "security": {},
  "build": {
    "upload": {
      "name": "target",
      "paths": [
        "${{ inputs.rust_workdir }}target/release/${{ steps.environments.outputs.project }}",
        "${{ inputs.rust_workdir }}target/default/report.xml",
        "${{ inputs.rust_workdir }}target/cobertura.xml"
      ]
    }
  },
  "containerize": {
    "download": {
      "name": "target",
      "paths": ["${{ inputs.rust_workdir }}target/"]
    }
  }
}
