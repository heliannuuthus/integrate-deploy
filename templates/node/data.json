{
  "workdir": "{{ inputs.node_workdir }}",
	"variables": {
		"inputs": [
			{
				"name": "node_workdir",
				"default": "./",
				"desc": "Specify all of node steps where are run",
				"type": "string",
				"required": "false"
			},
			{
				"name": "node",
				"required": "false",
				"default": "lts/*",
				"type": "string",
				"desc": "Specify node version, default: lts"
			},
			{
				"name": "build",
				"required": "false",
				"default": "build",
				"type": "string",
				"desc": "Specify build command"
			},
			{
				"name": "test",
				"required": "false",
				"type": "string",
				"desc": "Specify test command"
			}
		],
		"secrets": [],
		"outputs": []
	},
	"lint": {},
	"security": {}
}
